<?xml version='1.0' encoding='utf-8'?>
<widget id="com.veiss.clearthescreen" version="0.1.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Clear The Screen</name>
    <description>
        Clear screen game
    </description>
    <author email="janis.veiss93@gmail.com">
        Janis Veiss
    </author>
    <icon density="mdpi" height="57" platform="ios" src="www/img/icon.png" width="57" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="Orientation" value="portrait" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-admobpro" spec="^2.31.6" />
    <engine name="android" spec="^7.0.0" />
</widget>
